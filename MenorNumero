// Fazer um programa que ache o menor número entre 3 opções.

using System;
namespace ifelse
{
    class Program
    {
        static void Main(string[] args)
        {
                int a, b, c;

                String[] lista = Console.ReadLine().Split(' ');

                a = int.Parse(lista[0]);
                b = int.Parse(lista[1]);
                c = int.Parse(lista[2]);

                if (a < b && a < c)
                {
                    Console.WriteLine("O MENOR É O " + a);
                }

                else if (b < c)
                {
                    Console.WriteLine("O MENOR É O " + b);
                }

                else
                {
                    Console.WriteLine("O MENOR É O " + c);
                }
        }
    }
}
